<!DOCTYPE html>
<html lang="en">

{{>head}}


<body id="page-top">

  {{>nav-bar}}
  <div id="page-content">
    <section class="track-list-container bg-gray">
      <div class="container text-center">
         <div class="row">
         <div class="col-lg-8 mx-auto">
                <input style="width:85%;"type="search" id="search_field" name="search" placeholder="Search track..." >
                <div id="search_info">
                 Seacth results for:<span id="search_info_str"></span>
                <button id="clear_filter"class="close float-none" aria-label="Erase search" type="button" ><span aria-hidden="true">&times;</span></button>
                </div>
         </div>
         </div>

        
        <div class="bg-gray" id="trackList" style="max-height:600px; overflow-y: scroll; overflow-x: hidden; margin: 20px 0px;">
        Loading...
        </div>

         
         <div class="row">
        <div class="col-lg-8 mx-auto">
            <div class="page_refs">
                 <div id="page_info"></div>
            </div>
                <button type ="button"  id="prev_page" class='btn btn-primary'>&laquo; Previous page</button>
                <button type ="button" id="next_page"  class='btn btn-primary'>&raquo; Next page</button>
            <div  id='no_tracks' class = "center vis-text">No tracks were found.</div>
            <a class ="btn btn-primary float-right" href="/tracks/new">Add new track</a>
        </div>
        </div>
      </div>
    </section>
  </div>
  {{>footer}}
  <script src="/javascripts/trackui.js"></script>
  <script src="/javascripts/tracks.js"></script>
</body>

</html>
